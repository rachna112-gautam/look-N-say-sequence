#include <iostream>
using namespace std;

int main() {
	int n,i,k;
	cin>>n;
	int a[n][n];
	int j=0;
	k=n;
	while(j<n){
	for(i=j;i<n;i++)
	{
	   
	        a[j][i]=k;
	        a[i][j]=k;
	        
	    
	}j++;
	k--;
	}
	for(i=0;i<n;i++){
	    for(j=0;j<n;j++){
	        cout<<a[i][j];
	    }
	    cout<<endl;
	}
	
	return 0;
}
//input 
n=5;
//output
55555
54444
54333
54322
54321
